pipeline {
    // add your slave label name
    agent { label 'agent_name'}
    tools{
        maven 'maven-test'
    }
    stages {
        stage ('Checkout SCM') {

            steps {
          	    
	     checkout scm
            }
        }

        stage ('Build') {

            steps {
		sh 'mvn clean package'
            }
        }
        
        stage ('deploy') {

            steps {
               sshagent{['Agent_name']}{
	      sh "scp -o StrictHostKeyChecking-no target/maven-web-application.war  ec2-user@16.171.5.22:/opt/tomcat9/webapps/"
                 
	     
	     }   
            }
         }

      }  
    }

